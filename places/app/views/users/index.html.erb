<%= link_to 'New', new_user_path %>

<% @users.each do |user| %>
    <div>
        <h4><%= user.first_name %> <%= user.last_name %></h4>
        <p><%= user.email %></p>
        <img src="<%= user.picture %>" height="100">
        <%= link_to 'Edit', edit_user_path(user) %>
        <%= link_to 'Show', user_path(user) %>
    </div>
<% end %>